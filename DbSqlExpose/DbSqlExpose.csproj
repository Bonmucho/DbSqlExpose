<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>DbSqlExpose</Title>
    <Authors>Philipp Ape</Authors>
    <Description>Provides extension methods for a web application to execute SQL database queries.</Description>
    <PackageProjectUrl>https://github.com/Bonmucho/DbSqlExpose</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Bonmucho/DbSqlExpose</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.0" />
  </ItemGroup>

	<ItemGroup>
		<ProjectCapability Include="AspNetCore" />
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

</Project>
